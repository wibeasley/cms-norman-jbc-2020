#!/usr/bin/python
import os, sys
from wallaby import *

def main():
    print "tag-you-are-it"
    print "l-2"
        
    print "go forward"
    motor(0, 30)
    motor(3, 30)
    msleep(9500)
    
    print "go backwards"
    motor(0, -30)
    motor(3, -30)
        
    msleep(9000)

if __name__== "__main__":
    sys.stdout = os.fdopen(sys.stdout.fileno(),"w",0)
    main();
